<div class="icons-content">
  <h2>{{ section.settings.section_title }}</h2>
  <div class="icons-content__logo-list">
    {% for block in section.blocks %}
      <div class="icons-content__logo-list-item">
        <img src="{{ block.settings.logo.src | image_url }}" />
      </div>
    {% endfor %}
  </div>
</div>

{% schema %}

  {
  "name": "Icons Full Width",
  "class": "icons-full-width",
  "settings": [
    {
    "type": "text",
    "id": "section_title",
    "label": "Section Title"
    }
  ],
  "blocks": [
  {
  "type":"slider_item",
  "name":"Slider Item",
  "settings":[
    {
    "type": "image_picker",
    "id": "logo",
    "label": "Logo"
    }
  ]
  }
  ]
  }

{% endschema %}
